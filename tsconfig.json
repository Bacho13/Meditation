{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": ["./*"]
    },
    "jsx": "react-jsx"
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "app.d.ts",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "src/**/*",
    "src/app.d.ts"
  ]
}
